<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="shortcut icon" type="image/x-icon" href="{{ asset('css/images/favicon.ico') }}" />
        <link rel="stylesheet" href="{{ asset('css/style.css') }}" type="text/css" />
        <link rel="stylesheet" href="{{ asset('css/prettyCheckboxes.css') }}" type="text/css" />
        <link rel="stylesheet" href="{{ asset('css/customScroller.css') }}" type="text/css" />
        <script src="{{ asset('js/jquery-1.4.2.js') }}" type="text/javascript"></script>
        <script src="{{ asset('js/prettyCheckboxes.js') }}" type="text/javascript"></script>
        <script src="{{ asset('js/jquery.customScroller-1.2.js') }}" type="text/javascript"></script>
        <script src="{{ asset('js/js-func.js') }}" type="text/javascript"></script>
        <script src="{{ asset('js/jquery-2.2.0.min.js') }}" type="text/javascript"></script>
        {% block stylesheets %}{% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
